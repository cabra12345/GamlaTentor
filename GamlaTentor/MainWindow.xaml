<Window
    x:Class="GamlaTentor.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:GamlaTentor"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <TextBox
            x:Name="txtBox1"
            Width="120"
            Margin="268,0,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Text=""
            TextWrapping="Wrap" />
        <Button
            x:Name="btnBalans"
            Margin="204,280,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="btnBalans_Click"
            Content="Balanstest" />
        <Button
            x:Name="btnKö"
            Width="47"
            Margin="294,280,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="btnKö_Click"
            Content="Kö" />
        <Button
            x:Name="btnVänta"
            Margin="376,280,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="btnVänta_Click"
            Content="Väntetid" />
        <Button
            x:Name="btnRatings"
            Margin="469,280,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="btnRatings_Click"
            Content="Betygssättning" />
        <Button
            x:Name="btnTrender"
            Margin="608,280,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Trender" Click="btnTrender_Click" />
    </Grid>
</Window>
